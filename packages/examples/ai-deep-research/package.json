{
  "name": "@newintel/ai-deep-research",
  "version": "0.1.0",
  "description": "Deep research agent example powered by rxpress with Web UI",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node --import ./scripts/register-ts-node.mjs --enable-source-maps src/main.ts",
    "build": "tsc -p .",
    "start": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "node --import ./scripts/register-ts-node.mjs --test \"src/__tests__/**/*.test.ts\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "openai": "^5.7.0",
    "rxpress": "file:../../rxpress",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^22.8.6",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
